{
  "name": "hnpwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "lint:js": "eslint ./src ./server",
    "prebuild:local": "yarn build:clean",
    "prebuild:": "yarn build:clean",
    "build": "cross-env NODE_ENV=production ASSETS_URL=http://cdn.example.com/ webpack --config ./webpack/webpack.config.prod.js --progress",
    "build:local": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js --progress",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node --inspect=127.0.0.1:9230 ./start.js",
    "start:prod:local": "yarn build:local && cross-env PORT=8080 node ./start.prod.js",
    "start:prod": "yarn build && node ./start.prod.js"
  },
  "browserslist": [
    "Chrome >= 50",
    "Firefox >= 50",
    "Edge >= 12",
    "iOS >= 10",
    "Safari >= 10",
    "Opera >= 40"
